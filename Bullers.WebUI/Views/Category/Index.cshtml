@using Bullers.Entity.Concrete
@model List<Category>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-bordered">
    <tr>
        <th> ID</th>
        <th> Name</th>
        <th> Date</th>
        <th> Description</th>
    </tr>

    @foreach (var x in Model)
    {
        <tr>
            <td>@x.CategoryID</td>
           <td>@x.CategoryName</td>   
            <td>@x.Status</td>
        </tr>


    }
</table>
